<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="leftMenu" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.my.game.controllers.LeftMenuController">
   <children>
      <ToggleButton mnemonicParsing="false" onAction="#openCharacterWindow" prefHeight="50.0" prefWidth="100.0" text="%leftMenuButton.1">
         <toggleGroup>
            <ToggleGroup fx:id="leftMenuToggleGroup" />
         </toggleGroup>
      </ToggleButton>
      <ToggleButton mnemonicParsing="false" onAction="#openBackpackWindow" prefHeight="50.0" prefWidth="100.0" text="%leftMenuButton.2" toggleGroup="$leftMenuToggleGroup" />
      <ToggleButton mnemonicParsing="false" onAction="#openActionWindow" prefHeight="50.0" prefWidth="100.0" text="%leftMenuButton.3" textAlignment="CENTER" toggleGroup="$leftMenuToggleGroup" wrapText="true" />
      <ToggleButton mnemonicParsing="false" onAction="#openCityWindow" prefHeight="50.0" prefWidth="100.0" text="%leftMenuButton.4" toggleGroup="$leftMenuToggleGroup" />
      <ToggleButton mnemonicParsing="false" onAction="#openGoToWindow" prefHeight="50.0" prefWidth="100.0" text="%leftMenuButton.5" toggleGroup="$leftMenuToggleGroup" />
      <ToggleButton mnemonicParsing="false" onAction="#openHomeWindow" prefHeight="50.0" prefWidth="100.0" text="%leftMenuButton.6" toggleGroup="$leftMenuToggleGroup" />
      <ToggleButton mnemonicParsing="false" onAction="#openProperitiesWindow" prefHeight="50.0" prefWidth="100.0" text="%leftMenuButton.7" toggleGroup="$leftMenuToggleGroup" />
      <ToggleButton mnemonicParsing="false" onAction="#openGroupWindow" prefHeight="50.0" prefWidth="100.0" text="%leftMenuButton.8" textAlignment="CENTER" toggleGroup="$leftMenuToggleGroup" wrapText="true" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
